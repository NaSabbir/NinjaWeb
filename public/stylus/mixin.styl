// Colores
ColorRed 	= #C71A2B
ColorPtm    = #10B18D
ColorOrange = #FBA919
ColorCobal 	= #1d9b9e
ColorGreen  = #20c05c
ColorCoral 	= #F04F53
ColorBlue 	= #1378AE

.u-descendIcon
	color #999
	font-size 2.5em
	animation bounce 2s infinite linear
	position relative

.u-wrapper
	box-sizing border-box
	display block
	margin 0 auto
	max-width 960px
	position relative
	padding 0 15px

.u-inputNone
	display none

@keyframes bounce
	0%
	20%
	50%
	80%
	100%
		transform translateY(0)
	40%
		transform translateY(-30px)
	60%
		transform translateY(-15px)



