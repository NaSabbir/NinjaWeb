@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?-acobf8');
	src:url('../fonts/icomoon.eot?#iefix-acobf8') format('embedded-opentype'),
		url('../fonts/icomoon.woff?-acobf8') format('woff'),
		url('../fonts/icomoon.ttf?-acobf8') format('truetype'),
		url('../fonts/icomoon.svg?-acobf8#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"]:before, [class*=" icon-"]:before {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

